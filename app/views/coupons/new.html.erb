<h1>Coupon Form</h1>
<%= form_tag coupons_path do %>
  <%= label_tag :coupon_code, 'Coupon Code' %><br>
  <%= text_field_tag :'coupon[coupon_code]', value = nil, id: 'coupon_code' %><br>

  <%= label_tag :store, 'Store' %><br>
  <%= text_field_tag :'coupon[store]', value = nil, id: 'store' %><br>

  <%= submit_tag 'Submit Coupon' %>
<% end %>
